<table class="table table-striped table-dictionaries">
    <thead>
    <tr>
        <th class="w50">№</th>
        <th>Обозначение</th>
        <th>Название</th>
    </tr>
    </thead>
    <tbody>
    {% for dict in dicts %}
    <tr>
        <td class="w50">{{ loop.index + 1 }}</td>
        <td>{{ dict.shortName }}</td>
        <td>{{ dict.name }}</td>
    </tr>
    {% else %}
    <tr>
        <td colspan="3">Список пуст</td>
    </tr>
    {% endfor %}
    </tbody>
</table>